[[ literal ]]
  <!-- entry '[[title]]' -->
    <div class="article">
      <h2><a href="[[ link hrefonly=1 ]]">[[title]]</a></h2>
   <p class="info noprint">
       <span class="date">
         [[ date format="%dayname% %day% %monthname% %year% kl. %hour24%:%minute%" ]]
         [[ editlink format="Edit" prefix=" - " ]]
       </span>
   </p>
   [[introduction]]
      <p class="btn-more box noprint"><strong>[[more]]</strong></p>
      <p class="info noprint">
        <span class="user">[[ user field=emailtonick ]] |
        [[* [[trackbacklink]] | *]]
        [[ permalink text="&para;" title="Permanent link to '%title%' in the archives" ]] |
        [[commentlink]]
       [[tags prefix="| " ]]</span>
      </p>
            <hr class="noscreen" />
    </div>
[[ /literal ]]